    },
    {
        "astCommandId": "6fcb6fca74010b13e02894d5eea66e07fb0d2d2c:9:19",
        "astCommand": {
            "children": [
                {
                    "children": [
                        {
                            "children": [
                                {
                                    "children": [],
                                    "type": "BASH-LITERAL",
                                    "value": "cacertsFile=; for f in \"$JAVA_HOME/lib/security/cacerts\" \"$JAVA_HOME/jre/lib/security/cacerts\"; do if [ -e \"$f\" ]; then cacertsFile=\"$f\"; break; fi; done"
                                }
                            ],
                            "type": "SC-ECHO-ITEM"
                        }
                    ],
                    "type": "SC-ECHO-ITEMS"
                }
            ],
            "type": "SC-ECHO"
        }
    },
    {
        "astCommandId": "6fcb6fca74010b13e02894d5eea66e07fb0d2d2c:9:20",
        "astCommand": {
            "children": [
                {
                    "children": [
                        {
                            "children": [
                                {
                                    "children": [],
                                    "type": "BASH-LITERAL",
                                    "value": "if [ -z \"$cacertsFile\" ] || ! [ -f \"$cacertsFile\" ]; then echo >&2 \"error: failed to find cacerts file in $JAVA_HOME\"; exit 1; fi"
                                }
                            ],
                            "type": "SC-ECHO-ITEM"
                        }
                    ],
                    "type": "SC-ECHO-ITEMS"
                }
            ],
            "type": "SC-ECHO"
        }
    },
    {
        "astCommandId": "6fcb6fca74010b13e02894d5eea66e07fb0d2d2c:9:21",
        "astCommand": {
            "children": [
                {
                    "children": [
                        {
                            "children": [
                                {
                                    "children": [],
                                    "type": "BASH-LITERAL",
                                    "value": "trust extract --overwrite --format=java-cacerts --filter=ca-anchors --purpose=server-auth \"$cacertsFile\""
                                }
                            ],
                            "type": "SC-ECHO-ITEM"
                        }
                    ],
                    "type": "SC-ECHO-ITEMS"
                }
            ],
            "type": "SC-ECHO"
        }
    },
