                                {
                                    "children": [],
                                    "type": "BASH-LITERAL",
                                    "value": "if ! [ -d \"$JAVA_HOME\" ]; then echo >&2 \"error: missing JAVA_HOME environment variable\"; exit 1; fi"
                                }
                            ],
                            "type": "SC-ECHO-ITEM"
                        }
                    ],
                    "type": "SC-ECHO-ITEMS"
                }
            ],
